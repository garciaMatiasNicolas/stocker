<!DOCTYPE html>
<html lang="en">
    <%- include('../partials/head') %>
<body class="bg-dark">
    <%- include('../partials/navbar') %>
    <h1 class="text-white m-5">Table</h1>
    <table class="table table-dark table-striped w-75 m-5">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Price</th>
                <th scope="col">Image</th>
            </tr>
        </thead>
        <tbody>
            <% mockProducts.forEach(function(product) { %>
                <tr>
                    <td><%= product.name %></td>
                    <td><%= product.price %></td>
                    <td><%= product.photo %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>